{
  "name": "cloudflared-homepage",
  "version": "1.0.0",
  "description": "A user-friendly dashboard for Cloudflare Tunnel applications",
  "main": "build-index.js",
  "scripts": {
    "build": "node build-index.js",
    "start": "node build-index.js && echo 'Build complete'",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker-build": "docker build -t cloudflared-homepage .",
    "docker-run": "docker run -d -p 8080:80 -v $(pwd)/config.yaml:/usr/share/nginx/html/config.yaml -v $(pwd)/cloudflared-config:/etc/cloudflared cloudflared-homepage"
